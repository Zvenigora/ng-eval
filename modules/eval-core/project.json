{
  "name": "eval-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "modules/eval-core/src",
  "prefix": "zvenigora",
  "tags": [],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "modules/eval-core/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "modules/eval-core/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "modules/eval-core/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "modules/eval-core/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "modules/eval-core/**/*.ts",
          "modules/eval-core/**/*.html",
          "modules/eval-core/package.json"
        ]
      }
    }
  }
}
